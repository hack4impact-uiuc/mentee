{
  "version": 2,
  "name": "h4i-mentee-auth",
  "builds": [{ "src": "src/nowServer.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "src/nowServer.js" }],
  "env": {
    "INFRA_EMAIL": "@mentee_auth_email",
    "INFRA_CLIENT_ID": "@mentee_auth_client_id",
    "INFRA_CLIENT_SECRET": "@mentee_auth_client_secret",
    "INFRA_REFRESH_TOKEN": "@mentee_auth_refresh_token",
    "INFRA_MONGO_URI": "@mentee_auth_mongo_uri"
  }
}
