# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to version control

# MongoDB Configuration
MONGO_USER=your_mongo_username
MONGO_PASSWORD=your_mongo_password
MONGO_HOST=mongodb+srv://%s:%s@your-cluster.mongodb.net/%s?retryWrites=true&w=majority
MONGO_DB=your_database_name

# Third-party API Keys
IMGUR_KEY=your_imgur_api_key
SENDGRID_API_KEY=your_sendgrid_api_key
SENDER_EMAIL=your_sender_email@domain.com

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE=your_twilio_phone_number

# Firebase Configuration
GOOGLE_APPLICATION_CREDENTIALS="firebase_service_key.json"
FIREBASE_API_KEY=your_firebase_api_key

# Eight x Eight API
EIGHT_X_EIGHT_API_KEY=your_8x8_api_key
EIGHT_X_EIGHT_APP_ID=your_8x8_app_id
EIGHT_X_EIGHT_ENCODED_PRIVATE_KEY=your_8x8_private_key

# Test Environment Variables (for testing only)
TEST_ADMIN_PASSWORD=test_admin_password
TEST_MENTOR_PASSWORD=test_mentor_password
TEST_MENTEE_PASSWORD=test_mentee_password
TEST_GUEST_PASSWORD=test_guest_password
TEST_PARTNER_PASSWORD=test_partner_password
MENTOR_JWT_TOKEN=test_mentor_jwt_token
MENTEE_JWT_TOKEN=test_mentee_jwt_token
